{
  "name": "fluxion",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint --ext .ts \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "eslint --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:fix": "npm run format -- --write",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "seed:create": "typeorm-ts-node-commonjs migration:create",
    "seed:run": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js -d ./src/database/seedsDataSource.ts migration:run",
    "seed:revert": "ts-node -r tsconfig-paths/register src/database/seedsRevert.ts",
    "migration:create": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:create",
    "migration:run:latest": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js -d ./src/database/migrationsDataSource.ts migration:run",
    "migration:rollback": "node node_modules/typeorm/cli.js migration:revert",
    "migration:rollback:latest": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js -d ./src/database/migrationsDataSource.ts migration:revert",
    "db:schema:create": "ts-node -r tsconfig-paths/register ./scripts/database/createDBschema.ts",
    "prepare": "cd .. && husky install backend/.husky"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "argon2": "^0.41.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "commitlint": "^19.6.1",
    "dotenv": "^16.4.7",
    "husky": "^8.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
